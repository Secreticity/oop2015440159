
public class Point {

	public double x;
	public double y;
	
	public Point move(Point p) {
		double a, b;  //Temporary Variables
		Point pnt = new Point();

		//C : P2 = move(p1, p);
		//Java: P2 = P1.move(p);
		
		a = this.x + p.x;
		b = this.y + p.y;
		
		pnt.x = a;
		pnt.y = b;
		return pnt;
	}
}
